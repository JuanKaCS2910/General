@model Domain.Entities.ViewModel.ViewModelHistorico
@using PagedList.Mvc
@{
    ViewBag.Title = "Histórico";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Historial</h2>
    </div>
    <div class="col-lg-2">

    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="tabs-container">
                <ul class="nav nav-tabs">
                    <li id="litab1" class="active">
                        <a class="nav-link active" data-toggle="tab" href="#tab-1">Lista</a>
                    </li>
                    <li id="litab2">
                        <a class="nav-link" data-toggle="tab" href="#tab-2">Historial</a>
                    </li>
                    <li id="litab3">
                        <a class="nav-link" data-toggle="tab" href="#tab-3">Detalle</a>
                    </li>
                </ul>
            </div>

            <div class="tab-content">
                <div id="tab-1" class="tab-pane active">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Listado <small>De Pacientes</small></h5>
                            <div class="ibox-tools">
                                <a class="collapse-link">
                                    <i class="fa fa-chevron-up"></i>
                                </a>
                            </div>
                        </div>
                        <div class="ibox-content">
                            <div class="row">
                                <div class="ibox-title">
                                    <h5>Listado de Pacientes</h5>
                                </div>
                            </div>
                            <br />

                            <div class="row">
                                <div class="col-xs-12 col-md-12">
                                    <div class="col-xs-12 col-md-3">
                                        @Html.LabelFor(model => Model.FiltroPerson.TipodocumentoId, htmlAttributes: new { @class = "control-label input-style" })
                                    </div>
                                    <div class="col-xs-12 col-md-3">
                                        <div class="form-group">
                                            @Html.DropDownList("FiltroDocumentypeId", null, htmlAttributes: new { @class = "form-control-formulario" })
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3">
                                        @Html.LabelFor(model => Model.FiltroPerson.Nrodocumento, htmlAttributes: new { @class = "control-label input-style" })
                                    </div>
                                    <div class="col-xs-12 col-md-3">
                                        <div class="form-group">
                                            @Html.EditorFor(model => Model.FiltroPerson.Nrodocumento, new { htmlAttributes = new { @class = "form-control" } })
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xs-12 col-md-12">
                                    <div class="col-xs-12 col-md-3">
                                        <label class="control-label input-style" for="NombreFiltro">Ap. Paterno</label>
                                    </div>
                                    <div class="col-xs-12 col-md-6">
                                        @Html.EditorFor(model => Model.FiltroPerson.Apellidopaterno, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                    <div class="col-xs-12 col-md-3">
                                        <button class="btn btn-primary" id="btnFiltro">Buscar</button>
                                    </div>
                                </div>
                            </div>

                            <br />

                            <div class="row">
                                <div class="cols-xs-12 col-md-3">
                                    <table>
                                        <tr>
                                            <td class="dataTables_length" width="75px;">
                                                <label class="control-label input-style" for="NombreFiltro">Mostrar : </label>
                                            </td>
                                            <td width="75px;">
                                                <select id="tblPersonGrid_length" aria-controls="tblPersonGrid" class="form-control form-control-sm">
                                                    <option value="10">10</option>
                                                    <option value="25">25</option>
                                                    <option value="50">50</option>
                                                    <option value="100">100</option>
                                                </select>
                                            </td>
                                            <td width="10px;"></td>
                                            <td width="100px;">
                                                <label class="control-label input-style" for="NombreFiltro">registros.</label>
                                            </td>
                                        </tr>

                                    </table>

                                </div>
                                <div class="col-xs-12 col-md-6">

                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <label class="control-label input-style" for="NombreFiltro" id="tblPersonGrid_info">
                                        Página: @(Model.PersonaGrilla.PageCount < Model.PersonaGrilla.PageNumber ? 0 : Model.PersonaGrilla.PageNumber) de: @Model.PersonaGrilla.PageCount
                                    </label>
                                </div>
                            </div>

                            <div class="row">
                                <div class="cols-xs-12 col-md-12">
                                    <table id="tblPersonGrid" class="table table-striped table-bordered table-hover dataTables-examplePerson">
                                        <thead>
                                            <tr class="EstiloCabecera">
                                                <th class="col-xs-12 col-md-1">

                                                </th>
                                                <th class="col-xs-12 col-md-4">
                                                    Nombre Completo
                                                </th>
                                                <th class="col-xs-12 col-md-2">
                                                    @Html.DisplayNameFor(model => Model.PersonaGrilla.FirstOrDefault().Nrodocumento)
                                                </th>
                                                <th class="col-xs-12 col-md-1">
                                                    @Html.DisplayNameFor(model => Model.PersonaGrilla.FirstOrDefault().Nrotelefono)
                                                </th>
                                                <th class="col-xs-12 col-md-4">
                                                    @Html.DisplayNameFor(model => Model.PersonaGrilla.FirstOrDefault().Direccion)
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var person in Model.PersonaGrilla)
                                            {
                                                <tr>
                                                    <td class="col-xs-12 col-md-1">
                                                        <div>
                                                            <table>
                                                                <tr>
                                                                    <td class="col-xs-12 col-md-12">
                                                                        <a class="fa fa-search" onclick="PersonSelect(this)" id="btnEditPerson"
                                                                           style="color:#6A5ACD"
                                                                           data-assigned-id=@person.PersonaId></a>
                                                                    </td>

                                                                </tr>
                                                            </table>
                                                        </div>
                                                    </td>
                                                    <td class="col-xs-12 col-md-4">
                                                        @Html.DisplayFor(model => person.Apellidopaterno) @Html.DisplayFor(model => person.Apellidomaterno) , @Html.DisplayFor(model => person.Nombre)
                                                    </td>

                                                    <td class="col-xs-12 col-md-2">
                                                        @Html.DisplayFor(model => person.Nrodocumento)
                                                    </td>
                                                    <td class="col-xs-12 col-md-1">
                                                        @Html.DisplayFor(model => person.Nrotelefono)
                                                    </td>
                                                    <td class="col-xs-12 col-md-4">
                                                        @Html.DisplayFor(model => person.Direccion)
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                        <tfoot>
                                            <tr class="EstiloCabecera">
                                                <th class="col-xs-12 col-md-1">

                                                </th>
                                                <th class="col-xs-12 col-md-4">
                                                    Nombre Completo
                                                </th>
                                                <th class="col-xs-12 col-md-2">
                                                    @Html.DisplayNameFor(model => Model.PersonaGrilla.FirstOrDefault().Nrodocumento)
                                                </th>
                                                <th class="col-xs-12 col-md-1">
                                                    @Html.DisplayNameFor(model => Model.PersonaGrilla.FirstOrDefault().Nrotelefono)
                                                </th>
                                                <th class="col-xs-12 col-md-4">
                                                    @Html.DisplayNameFor(model => Model.PersonaGrilla.FirstOrDefault().Direccion)
                                                </th>
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>
                            </div>

                            <div class="row">
                                <div class="cols-xs-12 col-md-12" id="hiPaginado">
                                    @Html.PagedListPager(Model.PersonaGrilla, page => Url.Action("Index", new
                                    {
                                        page,
                                        sortOrder = ViewBag.CurrentSort,
                                        currentFilter = ViewBag.CurrentFilter
                                    }))
                                </div>

                            </div>

                        </div>
                    </div>
                </div>


                <div id="tab-2" class="tab-pane">
                    <div class="ibox-title">
                        <h5>Historial <small>Del Paciente</small></h5>
                        <div class="ibox-tools">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>
                        </div>
                    </div>
                    <div class="ibox-content">
                        <div class="row">
                            <div class="ibox-title">
                                <h5>Historial de Paciente</h5>
                            </div>
                        </div>
                        <br />

                        <div class="row">
                            <div class="col-xs-12 col-md-12">
                                <div class="col-xs-12 col-md-3">

                                    <label class="control-label input-style" for="TDocumentoHLabel">T. Documento</label>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        <input class="form-control text-box single-line" id="TDocumentoHistorico" name="TDocumentoHistorico" type="text" value="" readonly>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3">
                                    <label class="control-label input-style" for="NroDocumentoHLabel">Nro. Documento</label>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        <input class="form-control text-box single-line" id="NroDocumentoHistorico" name="NroDocumentoHistorico" type="text" value="" readonly>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-12 col-md-12">
                                <div class="col-xs-12 col-md-3">
                                    <label class="control-label input-style" for="NombreFiltro">Nombre Completo</label>
                                </div>
                                <div class="col-xs-12 col-md-9">
                                    <input class="form-control text-box single-line" data-val="true" data-val-required="El campo Nombre Completo es obligatorio." id="NombreCompletoHistorico" name="NombreCompletoHistorico" type="text" value="" readonly>
                                </div>

                            </div>
                        </div>

                        <br />

                        <div class="row">
                            <div class="col-xs-12 col-md-3">
                                <table>
                                    <tr>
                                        <td class="dataTables_length" width="75px;">
                                            <label class="control-label input-style" for="NombreFiltro">Mostrar : </label>
                                        </td>
                                        <td width="75px;">
                                            <select id="tblHistoricoGrid_length" aria-controls="tblHistoricoGrid" class="form-control form-control-sm">
                                                <option value="10">10</option>
                                                <option value="25">25</option>
                                                <option value="50">50</option>
                                                <option value="100">100</option>
                                            </select>
                                        </td>
                                        <td width="10px;"></td>
                                        <td width="100px;">
                                            <label class="control-label input-style" for="NombreFiltro">registros.</label>
                                        </td>
                                    </tr>
                                </table>
                            </div>
                            <div class="col-xs-12 col-md-6">

                            </div>
                            <div class="col-xs-12 col-md-3">

                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-12 col-md-12">
                                <table id="tblHistoricoGrid" class="table table-striped table-bordered table-hover dataTables-examplePerson">
                                    <thead>
                                        <tr class="EstiloCabecera">
                                            <th class="col-xs-12 col-md-1"></th>
                                            <th class="col-xs-12 col-md-3">Diagnóstico</th>
                                            <th class="col-xs-12 col-md-3">Observaciones</th>
                                            <th class="col-xs-12 col-md-3">Otros</th>
                                            <th class="col-xs-12 col-md-2">Fecha</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model.HistoricoGrilla != null)
                                        {
                                            foreach (var historico in Model.HistoricoGrilla)
                                            {
                                                <tr class="grid-1">
                                                    <td class="col-xs-12 col-md-1">
                                                        <div>
                                                            <table>
                                                                <tr>
                                                                    <td class="col-xs-12 col-md-6">
                                                                        <a class="fa fa-search" onclick="HistoricoSelect(this)" id="btnEditHistorico"
                                                                           style="color:#6A5ACD"
                                                                           data-assigned-id=@historico.HistoricoId></a>
                                                                    </td>
                                                                    <td class="col-xs-12 col-md-6">
                                                                        <a class="fa fa-minus-circle" onclick="DeleteHistorico(this)" id="btnElimHistorico" style="color:red;" data-assigned-id=@historico.HistoricoId></a>
                                                                    </td>
                                                                </tr>
                                                            </table>
                                                        </div>
                                                    </td>
                                                    <td class="col-xs-12 col-md-3">
                                                        @Html.DisplayFor(model => historico.Diagnostico)
                                                    </td>
                                                    <td class="col-xs-12 col-md-3">
                                                        @Html.DisplayFor(model => historico.Observaciones)
                                                    </td>
                                                    <td class="col-xs-12 col-md-3">
                                                        @Html.DisplayFor(model => historico.Otros)
                                                    </td>
                                                    <td class="col-xs-12 col-md-2">
                                                        @Html.DisplayFor(model => historico.Fechacreacion)
                                                    </td>
                                                </tr>
                                            }
                                        }

                                    </tbody>
                                    <tfoot>
                                        <tr class="EstiloCabecera">
                                            <th class="col-xs-12 col-md-1"></th>
                                            <th class="col-xs-12 col-md-3">Diagnóstico</th>
                                            <th class="col-xs-12 col-md-3">Observaciones</th>
                                            <th class="col-xs-12 col-md-3">Otros</th>
                                            <th class="col-xs-12 col-md-2">Fecha</th>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>
                        </div>

                        
                        <div class="row">
                            <div class="cols-xs-12 col-md-12" id="hiPaginadoHistorico">
                                @if (Model.HistoricoGrilla != null)
                                {
                                    @Html.PagedListPager(Model.HistoricoGrilla, page => Url.Action("Index", new
                                    {
                                        page,
                                        sortOrder = ViewBag.CurrentSort,
                                        currentFilter = ViewBag.CurrentFilter
                                    }))
                                }
                            </div>

                        </div>
                        

                    </div>
                </div>

                <div id="tab-3" class="tab-pane">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Registro <small>De Pacientes</small></h5>
                            <div class="ibox-tools">
                                <a class="collapse-link">
                                    <i class="fa fa-chevron-up"></i>
                                </a>
                            </div>
                        </div>
                        @*Grabar|Editar|Eliminar*@
                        <div class="ibox-title">
                            <div class="row">
                                <div class="col-xs-12 col-md-1 div-btn-img">
                                    <button class="btn-img">
                                        <img src="~/Content/Iconos/new.png" class="img-button" id="btnNew" title="Nuevo">
                                    </button>
                                </div>
                                <div class="col-xs-12 col-md-1 div-btn-img">
                                    <button class="btn-img">
                                        <img src="~/Content/Iconos/save.png" class="img-button" id="btnSave" title="Grabar">
                                    </button>
                                </div>
                                <div class="col-xs-12 col-md-9">
                                </div>
                            </div>
                        </div>

                        <div class="ibox-content">

                            <div class="row">
                                <div class="col-xs-12 col-md-3">
                                    <label class="control-label input-style" for="TipodocumentoId">T. Documento</label>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @Html.DropDownList("DocumentypeId", null, htmlAttributes: new { @class = "form-control-formulario" })
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <label class="control-label input-style" for="Nrodocumento">Nro. Documento</label>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        <div class="input-group">
                                            <input class="form-control text-box single-line" id="Documento" name="Documento" type="text" value=""><span class="input-group-addon" style="color:blue;"><i class="fa fa-search" id="hisearch" style="color:blue"></i></span>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xs-12 col-md-3">
                                    <label class="control-label input-style" for="Paciente">Paciente</label>
                                </div>
                                <div class="col-xs-12 col-md-9">
                                    <div class="form-group">
                                        <input class="form-control text-box single-line" disabled id="Paciente" name="Paciente" type="text" value="">
                                    </div>
                                </div>
                            </div>
                            <input id="PersonaId" name="PersonaId" type="hidden" value="">
                            <div class="row">
                                <div class="col-xs-12 col-md-3">
                                    @Html.LabelFor(model => Model.Historicos.Diagnostico, htmlAttributes: new { @class = "control-label input-style" })
                                </div>
                                <div class="col-xs-12 col-md-9">
                                    <div class="form-group">
                                        @Html.EditorFor(model => Model.Historicos.Diagnostico, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xs-12 col-md-3">
                                    @Html.LabelFor(model => Model.Historicos.Paquetes, htmlAttributes: new { @class = "control-label input-style" })
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @Html.EditorFor(model => Model.Historicos.Paquetes, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    @Html.LabelFor(model => Model.Historicos.Costo, htmlAttributes: new { @class = "control-label input-style" })
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @Html.EditorFor(model => Model.Historicos.Costo, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="ibox-title">
                                    <h5>Tratamientos</h5>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        <h4 style="color:red">Agente Térmico</h4>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-9">

                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @Html.CheckBoxFor(model => Model.Historicos.checkCaliente, new { disabled = "disabled", @checked = false })
                                        @*<input type="checkbox" class="form-check-input" id="checkCaliente">*@
                                        <label class="form-check-label" for="Historicos_checkCaliente">Compresa Caliente</label>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @Html.CheckBoxFor(model => Model.Historicos.checkFria, new { disabled = "disabled", @checked = false })
                                        @*<input type="checkbox" class="form-check-input" id="checkFria">*@
                                        <label class="form-check-label" for="Historicos_checkFria">Compresa Fría</label>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @*<input id="Historicos_checkContraste" name="Historicos_checkContraste" type="checkbox"/>*@
                                        @Html.CheckBoxFor(model => Model.Historicos.checkContraste, new { disabled = "disabled", @checked = "checked" })
                                        @*<input type="checkbox" class="form-check-input" id="checkContraste">*@
                                        <label class="form-check-label" for="Historicos_checkContraste">Contraste</label>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-3">

                                </div>
                            </div>

                            <hr />

                            <div class="row">
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        <h4 style="color:red">Agente Electrofísico</h4>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-9">

                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        <label class="form-check-label" for="exampleCheck1">Electroanalgesico</label>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @Html.EditorFor(model => Model.Historicos.descElectroanalgesico, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                        @*<input class="form-control text-box single-line" id="Historicos_Paquetes" name="Historicos.Paquetes" type="text" value="">*@
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @Html.CheckBoxFor(model => Model.Historicos.checkElectroestimulacion, new { disabled = "disabled" })
                                        @*<input type="checkbox" class="form-check-input" id="checkElectroestimulacion">*@
                                        <label class="form-check-label" for="Historicos_checkElectroestimulacion">Electroestimulacion</label>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @Html.EditorFor(model => Model.Historicos.descElectroestimulacion, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                        @*<input class="form-control text-box single-line" id="Historicos_Paquetes" name="Historicos.Paquetes" type="text" value="">*@
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @*<input type="checkbox" class="form-check-input" id="checkMagnetoterapia">*@
                                        @Html.CheckBoxFor(model => Model.Historicos.checkMagnetoterapia, new { disabled = "disabled" })
                                        <label class="form-check-label" for="Historicos_checkMagnetoterapia">Magnetoterapia</label>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @Html.EditorFor(model => Model.Historicos.descMagnetoterapia, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @Html.CheckBoxFor(model => Model.Historicos.checkUltrasonido, new { disabled = "disabled" })
                                        <label class="form-check-label" for="Historicos_checkUltrasonido">Ultrasonido</label>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @Html.EditorFor(model => Model.Historicos.descUltrasonido, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-xs-12 col-md-3">
                                    <div class="form-group">
                                        @Html.CheckBoxFor(model => Model.Historicos.checkTCombinada, new { disabled = "disabled" })
                                        <label class="form-check-label" for="Historicos_checkTCombinada">T. Combinada</label>
                                    </div>

@model Domain.Entities.Mantenimiento.EUsuario
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Form Login | Terapia</title>
    <link rel="stylesheet" href="~/Content/Login/login.css">
</head>
<body>
    <div class="login-box">
        <img class="avatar" src="~/Content/Login/img/logo.png" alt="Logo Terapia">
        <h1>Bienvenido</h1>
        <br />
        <form class="login-form" method="post">

            @Html.LabelFor(model => Model.CodUsuario, htmlAttributes: new { @class = "control-label" })
            @Html.EditorFor(model => Model.CodUsuario, new { htmlAttributes = new { @class = "form-control", placeholder = "Ingrese Usuario" } })
            <!--User Name
    <label for="username">Usuario</label>
    <input type="text" placeholder="Ingrese Usuario">-->

            @Html.LabelFor(model => Model.Contrasena, htmlAttributes: new { @class = "control-label" })
            @Html.EditorFor(model => Model.Contrasena, new { htmlAttributes = new { @class = "form-control", placeholder = "Contraseña" } })
            <!--Password
        <label for="password">Contraseña</label>
        <input type="password" placeholder="Contraseña">
    -->
            <br />
            <br />
            <br />
            
            <input type="submit" value="Ingrese">

            @*<a href="#">Lost your password</a><br>
        <a href="#">Don't have an account</a>*@

        </form>
    </div>
</body>
</html>
